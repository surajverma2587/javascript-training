{
  "name": "javascript-training",
  "version": "1.0.1",
  "description": "Training material for a JavaScript Marathon to a seasoned Software Engineer.",
  "author": "Suraj Verma",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/surajverma2587/javascript-training"
  },
  "type": "module",
  "scripts": {
    "test": "eslint . && jest --no-cache --verbose --coverage ./*",
    "test:jest": "jest --no-cache --verbose --coverage \".*.spec.js$\"",
    "test:watch": "jest --no-cache --verbose --watch \".*.spec.js$\"",
    "test:coverage": "jest --no-cache --coverage --verbose \".*.spec.js$\"",
    "test:solution": "jest \"src/solutions/*\" --no-cache --watch --verbose",
    "arrays": "jest --no-cache --verbose --watch \"arrays.test.js$\"",
    "control-flow": "jest --no-cache --verbose --watch \"control_flow.test.js$\"",
    "objects": "jest --no-cache --verbose --watch \"objects.test.js$\"",
    "strings": "jest --no-cache --verbose --watch \"strings.test.js$\"",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.5.4",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "common-tags": "^1.8.0",
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.18.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.2"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
